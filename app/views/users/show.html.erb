<h2>User Summary: <%= @user.username %></h2>
<br>
Joined: <%= @user.created_at.to_datetime %>


<section class = "user-table">
  <h3>Votes</h3>
  <table>
    <thead>
      <tr>
        <th>Media Title</th>
        <th>Created By</th>
        <th>Published</th>
        <th>Category</th>
        <th>Voted On</th>
      </tr>
    </thead>

    <tbody>

      <tr>
        <% @media_votes.each do |work| %>
        <td><%= work.title %></td>
        <td><%= work.creator %></td>
        <td><%= work.publication_year %></td>
        <td><%= work.category %></td>
        <td><%= work.created_at %></td>

      </tr>
      <% end %>

    </tbody>
  </table>
</section>

<main>
<h2>Log In</h2>

<%= form_with model: @user, url: login_path, method: :post do |f| %>
<div class="form-group">
<%= f.label :username, "Username"%>
<%= f.text_field :username,id:"username" class:"form-control"%>
</div>

<div class="form-group">
<%= f.submit 'Login', class: "btn btn-primary"%>
</div>

<% end %>
</main>

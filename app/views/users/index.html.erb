<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<ul>
  <li><h2>ALL USERS</h2></li>
  <% @users.each do |user| %>
    <ul>
      <li><h3><%= link_to user.name, user_path(user.id) %></h3></li>
      <li><h4><%= (user.created_at).to_date.strftime("Joined site %m/%d/%Y") %></h4></li>
    </ul>
  <% end %>
</ul>

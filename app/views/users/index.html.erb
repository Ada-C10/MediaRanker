<section class="row">
  <table style="width:100%" class="table">
    <tr class="header_font">
      <th>List of Users</th>
    </tr>

    <% @users.each do |user| %>
    <tr>
      <td><%= link_to "#{user.username}", user_path(user.id) %></td>
    </tr>
    <% end %>
  </table>

  <%= link_to "Back to media list", root_path, class: "btn btn-secondary" %>
</section>

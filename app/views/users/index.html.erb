<h2>List of Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.name, user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "Back to Media List", root_path, class: "btn btn-secondary" %>

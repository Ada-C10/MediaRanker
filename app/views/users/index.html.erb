<main>
  <h2>List of Users</h2>
<table class="table">
<thead>
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
</thead>
<tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.user_name %></td>
      <td>Votes</td>
      <td><%= user.created_at %></td>
    </tr>
  <% end %>
</tbody>

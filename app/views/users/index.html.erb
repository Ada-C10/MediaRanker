
<div class="user-index">
<h2> List of Users </h2>

<table class="table">

  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>

<% @users.each do |user| %>

  <tr>
    <td> <%= link_to user.name, user_path(user.id) %> </td>
    <td> <%= user.votes.length %> </td>
    <td> <%= user.created_at %> </td>
  </tr>

<% end %>

</table>
</div>

<h1>List of Users</h1>
<table style="width:100%">
  <tr class="header_font">
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
  <%@users.each do |user| %>
  <tr>
    <td><%=link_to user.username, user_path(user.id) %></td>
    <td><%=user.votes.count %></td>
    <td><%=user.created_at%></td>
  </tr>
  <%end%>
</table>

<article>
  <h2 class = "users_list">List of Users</h2>

  <section>
    <table class="table">
      <thead>
        <tr>
          <th class = "table_grid">Username</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td>
              <%= link_to user.username, user_path(user.id) %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </section>

  <button type="button">
  <%= link_to "Back to Media List", root_path %>
  </button>
  
</article>

<h2><strong>List of Users</strong></h2>

<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>

  <tbody>
    <tr>
        <% @users.each do |u| %>
        <td> <%= link_to "#{u.username}", user_path(u.id)%></td>
        <td><%= u.votes.count %></td>
        <td><%= u.created_at.strftime("%b %d %Y") %></td>
    </tr>
    <% end %>
  </tbody>
</table>


<%= link_to "Back to Media List", root_path, class: "btn btn-primary" %>

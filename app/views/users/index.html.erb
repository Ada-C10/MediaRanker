<main>
    <h2>List of Users</h2>
<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <% @users.each do |user| %>
        <td><%= link_to user.username, user_path(user.id)%></td>
        <td><%= user.votes.length %></td>
        <td><%= user[:created_at].strftime('%b %d, %Y')%></td>
      </tr>
        <% end %>
  </tbody>
</table>

<a class="btn btn-secondary" href="/">Back to Media List</a>

  </main>

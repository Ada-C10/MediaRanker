<h3>List of Users</h3>

<table cellpadding="10">
<thead>
  <tr>
    <td><strong>Username</strong></td>
    <td><strong>Votes</strong></td>
    <td><strong>Date Joined</strong></td>
  </tr>
  </thead>

<tbody>
  <% @user.each do |u| %>
  <tr>
    <td><%= link_to "#{u.user_name}", user_path(u.id) %></td>
    <td><%= u.votes.count %></td>
    <td><%= u.created_at.strftime("%b %d %Y") %></td>

    <% end %>
</tr>
</tbody>

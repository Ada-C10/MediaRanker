

<table style="width:100%" class="table">
  <tr class="header_font">
    <th>Votes</th>
    <th>Title</th>
    <th>Created By</th>
    <th>Published</th>
    <th>Upvote</th>
  </tr>

<% @works.each do |work| %>
  <tr>
    <td>votes goes here</td>
    <td><%=link_to work.title, work_path(work.id)%></td>
    <td><%=work.creator%></td>
    <td><%=work.published_year%></td>
    <td>upvote button goes here</td>
  </tr>
<% end %>
</table>

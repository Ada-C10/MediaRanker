

<table style="width:100%" class="table">
  <tr class="header_font">
    <th>Votes</th>
    <th>Title</th>
    <th>Created By</th>
    <th>Published</th>
    <th>Upvote</th>
  </tr>

<% @works.each do |work| %>
  <tr>
    <td><%= work.votes.count%></td>
    <td><%=link_to work.title, work_path(work.id)%></td>
    <td><%=work.creator%></td>
    <td><%=work.published_year%></td>
    <td><div class="buttonContainer"><%= link_to "Vote", work_votes_path(work.id), method: :post, class: "button" %></div></td>
  </tr>
<% end %>
</table>

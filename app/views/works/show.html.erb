<h2><%= @work.title %> </h2>

<p> Created by: <%= @work.creator %> </p>

<p> Published: <%= @work.pub_year %> </p>

<p> <%= @work.description %> </p>


<div class="button-list">
  <%= link_to "Back to media ranks", root_path, class: "btn btn-primary" %>
  <%= link_to "Edit", edit_work_path, class: "btn btn-primary" %>
  <%= link_to "Upvote", root_path, class: "btn btn-primary" %>
  <%= link_to "Delete", work_path, class: "btn btn-primary", method: :delete %>
</div>

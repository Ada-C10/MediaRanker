<section class = "work-details">
  <h2><%= @work.title %></h2>
  <p>Created by: <%= @work.creator %></p>
  <p>Published: <%= @work.published_year %></p>
  <p><%= @work.description%></p>
  <p>
    <button type="button" class="btn btn-info">Back to media ranks</button>
    <button type="button" class="btn btn-info">Edit</button>
    <button type="button" class="btn btn-info">Upvote</button>
    <button type="button" class="btn btn-danger">Delete</button>
  </p>
</section>

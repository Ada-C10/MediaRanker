<%= form_with model: @work do |f| %>
<h2> <%= form_title %> </h2>
<p>
<%= f.label :category %>
<%= f.text_field :category %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :creator %>
<%= f.text_field :creator %>

<%= f.label :publication_year %>
<%= f.text_field :publication_year %>

<%= f.label :Description %>
<%= f.text_field :description %>
</p>
<p><%= f.submit action_type, class: "btn btn-primary" %></p>
<%end%>

<table>
  <tr>
    <th>Votes</th>
    <th>Title</th>
    <th>Created By</th>
    <th>Published</th>
    <th>Upvote</th>
  </tr>

  <% model.each do |work|%>
  <tr>
    <td>(vote num)</td>
    <td>
      <%= link_to work_path(work.id) do %>
      <%= work.title %>
      <% end %>
    </td>
    <td><%= work.creator %></td>
    <td><%= work.publication_year %></td>
    <td>(vote link)</td>
  </tr>
  <% end %>
</table>

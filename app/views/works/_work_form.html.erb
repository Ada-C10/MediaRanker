<%= action_title %>

<%= form_with model: model do |f| %>

  <%= f.label :category %>
  <%= f.text_field :category %>
  <br>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :creator %>
  <%= f.text_field :creator %>
  <br>

  <%= f.label :publication_year %>
  <%= f.text_field :publication_year %>
  <br>

  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>

  <%= f.submit action_submit %>
<% end %>

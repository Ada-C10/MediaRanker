<section>
  <h2>Log In</h2>

  <%= form_with method: :post, url: sessions_path do |f| %>
  <div class="form-group">
    <%= f.label :username%>
    <div class="col-md-8" >
      <%= f.text_field :username, class: "form-control"%>
    </div>
  </div>
  <button type="submit" class="btn btn-primary">  Log In </button>
  <% end %>

</section>
